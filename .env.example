# Supabase Configuration
# Copy this file to .env and fill in your values

# Get these from: https://app.supabase.com/project/YOUR_PROJECT/settings/api
VITE_SUPABASE_URL=your-project-url
VITE_SUPABASE_ANON_KEY=your-anon-key

# Database Direct Connection (for auto-migrations)
# Get password from: https://app.supabase.com/project/YOUR_PROJECT/settings/database
VITE_DB_HOST=db.your-project-ref.supabase.co
VITE_DB_PORT=5432
VITE_DB_NAME=postgres
VITE_DB_USER=postgres
VITE_DB_PASSWORD=your-database-password
